{
    "name": "react-isomorphic-boilerplate",
    "version": "1.0.0",
    "description": "An universal React isomorphic boilerplate for building server side render web app",
    "scripts": {
        "dev": "NODE_ENV=development BABEL_ENV=development nodemon build/server.dev.js --watch server",
        "build": "npm run clean && NODE_ENV=production webpack --config ./build/webpack.prod.config.js --progress",
        "clean": "rm -rf ./dist && rm -f ./server.js",
        "start": "node ./server.js"
    },
    "dependencies": {
        "ejs": "^2.5.5",
        "koa": "^2.0.0",
        "koa-bodyparser": "^3.2.0",
        "koa-convert": "^1.2.0",
        "koa-csrf": "^3.0.0",
        "koa-json": "^2.0.0",
        "koa-logger": "^2.0.0",
        "koa-router": "^7.0.0",
        "koa-static": "^3.0.0",
        "koa-views": "^5.0.0",
        "react": "^15.3.1",
        "react-bootstrap": "^0.30.3",
        "react-bootstrap-datetimepicker": "0.0.22",
        "react-dom": "^15.3.1",
        "react-redux": "^4.4.5",
        "react-router": "^3.0.0",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "superagent": "^2.2.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.4.1",
        "babel-core": "^6.13.2",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-polyfill": "^6.13.0",
        "babel-preset-es2015": "^6.13.2",
        "babel-preset-react": "^6.11.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.16.0",
        "css-loader": "^0.24.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "json-loader": "^0.5.4",
        "koa-webpack-dev-middleware": "^1.3.0",
        "koa-webpack-hot-middleware": "^1.0.3",
        "node-sass": "^3.9.0",
        "nodemon": "^1.11.0",
        "null-loader": "^0.1.1",
        "postcss-loader": "^0.11.1",
        "progress-bar-webpack-plugin": "^1.9.0",
        "redux-devtools": "^3.3.1",
        "redux-devtools-dock-monitor": "^1.1.1",
        "redux-devtools-log-monitor": "^1.0.11",
        "sass-loader": "^4.0.1",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.1"
    },
    "author": "Chikara Chan",
    "repository": {
        "type": "git",
        "url": "https://github.com/chikara-chan/react-isomorphic.git"
    },
    "license": "MIT"
}
